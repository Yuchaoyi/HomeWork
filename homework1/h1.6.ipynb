    "lst=eval(input('请输入一个序列：'))\n",
    "new_lst=list()\n",
    "for i in range(len(lst)-1,-1,-1):\n",
    "    new_lst.append(lst[i])\n",
    "print('利用for输出',new_lst)\n",
    "print('*'*50)\n",
    "new_lst.clear()\n",
    "i=len(lst)-1\n",
    "while i>=0:\n",
    "    new_lst.append(lst[i])\n",
    "    i=i-1\n",
    "print('利用while输出',new_lst)\n"
