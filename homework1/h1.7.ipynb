   "source": [
    "s=input('请输入一个字符串：')\n",
    "for i in range(len(s)):\n",
    "    if i==len(s)-1:\n",
    "        break;\n",
    "    if s[i]==s[i+1]:\n",
    "        print('S包含由两个或两个以上连续出现的相同字符组成的字符串')\n",
    "        break\n",
    "if i==len(s)-1:\n",
    "    print('S不包含由两个或两个以上连续出现的相同字符组成的字符串')"
   ]
  }
