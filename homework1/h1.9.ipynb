   "source": [
    "number=eval(input('请输入一个要求三次方根的数字：'))\n",
    "def newton_cubic_root(number):\n",
    "    x=number\n",
    "    while True:\n",
    "        next_x=(2*x**3+number)/(3*x**2)\n",
    "        if abs(next_x-x)<1e-6:\n",
    "            return next_x\n",
    "        x=next_x\n",
    "root=newton_cubic_root(number)\n",
    "print(f'{number}的三次方根接近于{root}')"
   ]
  }
