   "source": [
    "num=eval(input('请输入一个大于1的数，我可以判断它是否为质数：'))\n",
    "j=0\n",
    "if num<=1:\n",
    "    print('该数小于或等于1，不为质数哦！')\n",
    "else:\n",
    "    for i in range(2,num+1):\n",
    "        if num%i==0:\n",
    "            j+=1\n",
    "        if j==2:\n",
    "            print(f'{num}不是质数')\n",
    "            break\n",
    "    if j==1:\n",
    "        print(f'{num}是质数')\n",
    "        "
   ],
